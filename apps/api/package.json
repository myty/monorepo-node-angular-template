{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "main": "src",
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.4.2",
    "npm": ">= 8.5.0"
  },
  "scripts": {
    "db:start": "npx ts-node ../../node_modules/scripts/docker-db.ts db -s -n mysql-db",
    "db:migrate": "npm run db:start && prisma migrate deploy",
    "test": "npm run lint && npm run compile && npm run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "npm run db:migrate && ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib/",
    "mocha": "mocha --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc",
    "prisma": "prisma"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.11",
    "@feathersjs/authentication-local": "^4.5.11",
    "@feathersjs/authentication-oauth": "^4.5.11",
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio": "^4.5.11",
    "@feathersjs/transport-commons": "^4.5.12",
    "@prisma/client": "^3.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-prisma": "^0.3.1",
    "helmet": "^4.6.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/serve-favicon": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "axios": "^0.26.1",
    "eslint": "^8.6.0",
    "mocha": "^9.1.3",
    "prisma": "^3.11.0",
    "shx": "^0.3.4",
    "ts-dev": "^2.1.14",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
